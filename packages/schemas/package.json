{
  "name": "@nihilog/schemas",
  "version": "0.0.1",
  "description": "Shared schemas and types for nihilog monorepo",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@nihilog/code": "workspace:*",
    "@nihilog/db": "workspace:*",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "private": true
}